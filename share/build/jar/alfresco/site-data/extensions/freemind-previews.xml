<extension>
    <modules>
    
        <!--
            Customization modules for adding i18n labels and importing client-side dependencies for
            the web previewer plugins
        -->
        
        <module>
            <id>Freemind Viewer</id>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.components.preview</targetPackageRoot>
                    <sourcePackageRoot>freemind.custom.components.preview.freemind</sourcePackageRoot>
                </customization>
            </customizations>
            <auto-deploy>false</auto-deploy>
        </module>
        
        <module>
            <id>Freemind Configuration</id>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.components.preview</targetPackageRoot>
                    <sourcePackageRoot>freemind.custom.components.preview.freemind-config</sourcePackageRoot>
                </customization>
            </customizations>
            <auto-deploy>false</auto-deploy>
        </module>
     
    </modules>
</extension>